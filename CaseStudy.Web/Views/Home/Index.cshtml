@model List<string>
@{
    ViewData["Title"] = "Home Page";
}

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">line</th>
            <th scope="col">text</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 1; i <= Model.Count; i++)
        {
            <tr>
                <th scope="row">@i</th>
                <td>@Model[i - 1]</td>
            </tr>

        }

    </tbody>
</table>


